---
export interface Props {
  title?: string;
  subtitle?: string;
}
const { title, subtitle } = Astro.props as Props;
---

<section
  class="container text-center p-10 mx-auto text-transparent bg-clip-text bg-gradient-to-r from-pink-700 to-purple-400"
>
  <h1>{title}</h1>
  <p>{subtitle}</p>
</section>
